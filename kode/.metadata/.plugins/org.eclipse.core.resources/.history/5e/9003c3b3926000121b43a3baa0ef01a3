img {
	width: 200 px;
	float: left;
}

/***************
 * CSS3-selectors
 ***************/

/*Velger alle img-elementer på siden hvis src-atributt starter med "images"*/
img[src^="images"]{
	border: 5px solid orange;
}

/*Velger alle img-elementer på siden hvis src-atributt starter med "images"*/
img[src$="tiguan"]{
	border: 5px solid blue;
}
